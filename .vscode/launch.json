{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter (Chrome - Clean State)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome"
      ]
    },
    {
      "name": "Flutter (Chrome - Persistent Storage)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--web-port=9090",
        "--web-hostname=localhost",
        "--web-browser-flag=--user-data-dir=${env:HOME}/flutter_chrome_profile"
      ]
    }
  ]
}
